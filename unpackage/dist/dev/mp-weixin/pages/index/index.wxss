
page {
	width: 750rpx;
    font-size: 24rpx;
    height: 100vh;
	/* height: 100%; */
    font-family: PingFangSC-Regular,Helvetica,Arial,sans-serif;
	box-sizing: border-box;
	/* background: url('https://static.supercreator.cn/tarot/background3.png') #54285b no-repeat; */
	background-position: top;
	/* background-size: 100% auto; */
	background-size: cover;
	background-origin: content-box;
	overflow:hidden;
}
.content {
	display: flex;
	flex-direction: column;
	align-items: center;
	/* justify-content: center; */
	width: 750rpx;
	height: 100%;
	max-height: 100vh;
	position:relative;
	box-sizing: border-box;
}
@-webkit-keyframes btnAnimate {
0%,to {-webkit-transform:scale(1);transform:scale(1);}
50% {-webkit-transform:scale(1.2);transform:scale(1.2);}
}
@keyframes btnAnimate {
0%,to {-webkit-transform:scale(1);transform:scale(1);}
50% {-webkit-transform:scale(1.2);transform:scale(1.2);}
}
.flexCol, .flexRow {
    display: flex;
    flex-direction: row;
}
.page_bg {
	position:absolute;
	/* top:var(--status-bar-height); */
	top: 0;
	left:50%;
	width:750rpx;
	height:1624rpx;
	/* max-height: 100vh; */
	-webkit-transform:translateX(-50%);
	transform:translateX(-50%);
	/* opacity: .85; */
}
.taro-img {
    display: inline-block;
    overflow: hidden;
    position: relative;
    font-size: 0;
    width: 640rpx;
    height: 480rpx;
}
.taro-img__mode-scaletofill {
    object-fit: contain;
    width: 100%;
    height: 100%;
}
.content .titleContent {
	margin-top:322rpx;
	/* display:flex; */
}
.content .titleContent .image1,.content .titleContent .image2 {
	display:block;height:128rpx;margin:0 auto;background:0 0;
}
.content .titleContent .image2 {height:60rpx;margin-top:.12rem;}
.content .beginContent .image1,.content .titleContent .image3 {display:block;height:190rpx;margin:0 auto;margin-top:60rpx;background:0 0;}
.content .beginContent .image1 {margin-top: 30rpx;}
.content .beginContent .tips{color:#9a8e8e;margin-top:40rpx;text-align:center;z-index: 1;position:relative; letter-spacing: 2rpx; justify-content: center; align-items: center;}
.content .beginContent .beginBtn {position:relative;
margin:30rpx auto 0;
width:460rpx;
height:78rpx;
line-height: 78rpx;
letter-spacing: 6rpx;
color:#333;
z-index:2;
background: url("https://static.supercreator.cn/tarot/start-btn.png") no-repeat;
background-position:50%;
background-size:100% 100%;
-webkit-animation:btnAnimate 1.5s linear .01s infinite;
animation:btnAnimate 1.5s linear .01s infinite;
}
.content .footer {
	margin-top:120rpx;
	padding:20rpx 0 20rpx;
	display:flex;
	flex-direction:column;
	align-items:center;
	color:#9a8e8e;
	z-index: 1;
}
.content .footer .footer-item {text-align:center;flex:1;cursor:pointer;margin:.8rem .4rem;}
.content .footer .myOrder {font-size:.6rem;color:#d6b47d;text-decoration:underline;margin-top:.44rem;}
.content .company {margin-top:20rpx;font-size:24rpx;}


